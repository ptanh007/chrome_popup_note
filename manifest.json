{
  "manifest_version": 2,

  "name": "Chrome Popup mess",
  "description": "Create popup per 5min",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
	"identity",
	"storage",
    "tabs",
    "storage",
    "<all_urls>",
    "identity",
    "https://*.googleapis.com/*",
	"https://www.googleapis.com/auth/documents"
  ]
}